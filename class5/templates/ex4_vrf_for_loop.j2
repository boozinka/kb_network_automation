{% for vrf_dict in vrf_list %}
vrf definition {{ vrf_dict.vrf_name }}
 rd {{ vrf_dict.rd_num }}
 !
 {%- if vrf_dict.ipv4_enable %}
 address-family ipv4
  route-target export {{ vrf_dict.rd_num }}
  route-target import {{ vrf_dict.rd_num }}
 exit-address-family
 !
 {%- endif %}
 {%- if vrf_dict.ipv6_enable %}
 address-family ipv6
  route-target export {{ vrf_dict.rd_num }}
  route-target import {{ vrf_dict.rd_num }}
 exit-address-family
 {%- endif %}
{% endfor %}
